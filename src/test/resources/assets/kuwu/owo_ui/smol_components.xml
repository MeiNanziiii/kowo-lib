                                        <box id="expando-box">
                                            <color>aqua</color>
                                            <fill>true</fill>

                                            <sizing>
                                                <horizontal method="fixed">20</horizontal>
                                                <vertical method="fill">100</vertical>
                                            </sizing>
                                        </box>

                                        <box>
                                            <color>red</color>
                                            <fill>true</fill>

                                            <sizing>
                                                <horizontal method="expand">50</horizontal>
                                                <vertical method="fill">100</vertical>
                                            </sizing>
                                        </box>

                                        <box>
                                            <color>dark-purple</color>
                                            <fill>true</fill>

                                            <sizing>
                                                <horizontal method="expand">50</horizontal>
                                                <vertical method="fill">100</vertical>
                                            </sizing>
                                        </box>
                                    </children>

                                    <horizontal-alignment>center</horizontal-alignment>

                                    <surface>
                                        <vanilla-translucent/>
                                    </surface>

                                    <sizing>
                                        <horizontal method="fixed">100</horizontal>
                                        <vertical method="fixed">15</vertical>
                                    </sizing>
                                </flow-layout>

                                <slim-slider direction="horizontal" id="expando-slider">
                                    <min>5</min>
                                    <value>20</value>
                                    <max>60</max>
                                    <step-size>1</step-size>

                                    <sizing>
                                        <horizontal method="fixed">80</horizontal>
                                    </sizing>

                                    <margins>
                                        <all>5</all>
                                    </margins>
                                </slim-slider>
                            </children>

                            <horizontal-alignment>center</horizontal-alignment>

                            <surface>
