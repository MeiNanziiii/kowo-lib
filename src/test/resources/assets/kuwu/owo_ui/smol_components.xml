                    </sizing>
                </text-area>
            </children>

            <gap>15</gap>

            <surface>
                <vanilla-translucent/>
            </surface>

            <vertical-alignment>center</vertical-alignment>
            <horizontal-alignment>center</horizontal-alignment>
        </flow-layout>
    </components>

    <templates>
        <template name="inset-panel">
            <flow-layout direction="horizontal">
                <children/>

                <sizing>
                    <horizontal method="fill">20</horizontal>
                    <vertical method="fixed">50</vertical>
                </sizing>

                <surface>
                    <panel-with-inset>{{border-size}}</panel-with-inset>
                </surface>
            </flow-layout>
        </template>
    </templates>
